{
  "name": "medable-test",
  "version": "1.0.0",
  "main": "server.js",
  "scripts": {
    "lint": "eslint ./server",
    "build-doc": "apidoc -i ./server/api -o ./docs -f .js",
    "start": "npm run build-doc && babel-node server.js",
