  },
  "dependencies": {
    "async": "^0.9.2",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.3",
    "bson": "^1.0.5",
    "cachegoose": "^4.0.1",
    "compression": "^1.7.2",
    "cors": "^2.5.2",
    "express": "^4.16.2",
    "glob": "^7.1.3",
    "helmet": "~0.2.1",
    "htmlparser2": "^3.8.3",
    "http-status": "^1.0.1",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.15.0",
    "method-override": "^2.3.10",
    "moment": "^2.22.2",
    "mongoose": "^4.6.0",
    "request": "^2.83.0",
    "response-time": "^2.3.2",
    "spdy": "^3.4.7"
  },
  "devDependencies": {
    "@babel/cli": "^7.1.5",
    "@babel/core": "^7.1.6",
    "@babel/node": "^7.0.0",
    "@babel/preset-env": "^7.1.6",
    "apidoc": "^0.17.7",
    "babel-core": "^7.0.0-bridge.0",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "eslint": "^4.9.0",
    "jest": "^23.6.0",
    "nodemon": "^1.12.1",
    "supertest": "^3.3.0"
